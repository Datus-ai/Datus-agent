{
  "mcpServers": {
    "snowflake_pip": {
      "command": "uvx",
      "args": [
        "mcp_snowflake_server",
        "--account",
        "<your_account>",
        "--warehouse",
        "COMPUTE_WH_PARTICIPANT",
        "--user",
        "<your_username>",
        "--password",
        "<your_password>",
        "--role",
        "PUBLIC",
        "--database",
        "<your_database>",
        "--schema",
        "<your_schema>",
         "--log_level",
         "DEBUG"
      ]
    },
    "mysql": {
      "command": "uv",
      "args": [
        "run",
        "mysql_mcp_server"
      ],
      "env": {
        "MYSQL_HOST": "localhost",
        "MYSQL_PORT": "3306",
        "MYSQL_USER": "root",
        "MYSQL_PASSWORD": "12345678",
        "MYSQL_DATABASE": "fly"
      }
    }
  }
}
